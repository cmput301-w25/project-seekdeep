<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cell_constraint_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/cell_date"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:text="31"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cell_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="emotionn"
        app:layout_constraintEnd_toEndOf="@+id/cell_date"
        app:layout_constraintStart_toStartOf="@+id/cell_date"
        app:layout_constraintTop_toBottomOf="@+id/cell_date" />

</androidx.constraintlayout.widget.ConstraintLayout>